  {
    "poem_id": 16366,
    "author": "Толстой Алексей Константинович",
    "title": "Дробится, и плещет, и брызжет волна…"
  },
  {
    "poem_id": 16367,
    "author": "Толстой Алексей Константинович",
    "title": "Мой строгий друг, имей терпенье…"
  },
  {
    "poem_id": 16368,
    "author": "Толстой Алексей Константинович",
    "title": "Темнота и туман застилают мне путь…"
  },
  {
    "poem_id": 16369,
    "author": "Толстой Алексей Константинович",
    "title": "Крымские очерки 14 («Привал. Дымяся, огонек…»)"
  },
  {
    "poem_id": 16370,
    "author": "Толстой Алексей Константинович",
    "title": "Благовест"
  },
  {
    "poem_id": 16371,
    "author": "Толстой Алексей Константинович",
    "title": "Роман Галицкий"
  },
  {
    "poem_id": 16372,
    "author": "Толстой Алексей Константинович",
    "title": "А.М. Жемчужникову («Мы тебя субботним днем…»)"
  },
  {
    "poem_id": 16374,
    "author": "Толстой Алексей Константинович",
    "title": "Осень. Обсыпается весь наш бедный сад…"
  },
  {
    "poem_id": 16375,
    "author": "Толстой Алексей Константинович",
    "title": "Крымские очерки 6 («Туман встает на дне стремнин…»)"
  },
  {
    "poem_id": 16376,
    "author": "Толстой Алексей Константинович",
    "title": "В.А. Арцимовичу («Ты властитель всех сердец…»)"
  },
  {
    "poem_id": 16377,
    "author": "Толстой Алексей Константинович",
    "title": "Часто от паштета корка…"
  },
  {
    "poem_id": 16378,
    "author": "Толстой Алексей Константинович",
    "title": "М.М. Стасюлевичу («Скорее на небе луну…»)"
  },
  {
    "poem_id": 16379,
    "author": "Толстой Алексей Константинович",
    "title": "Песня о Гаральде и Ярославне"
  },
  {
    "poem_id": 16380,
    "author": "Толстой Алексей Константинович",
    "title": "Друзья, вы совершенно правы…"
  },
  {
    "poem_id": 16381,
    "author": "Толстой Алексей Константинович",
    "title": "И у меня был край родной когда-то…"
  },
  {
    "poem_id": 16382,
    "author": "Толстой Алексей Константинович",
    "title": "Что за время, что за нравы!.."
  },
  {
    "poem_id": 16383,
    "author": "Толстой Алексей Константинович",
    "title": "Мадонна Рафаэля"
  },
  {
    "poem_id": 16384,
    "author": "Толстой Алексей Константинович",
    "title": "Крымские очерки 3 («Всесильной волею аллаха…»)"
  },
  {
    "poem_id": 16385,
    "author": "Толстой Алексей Константинович",
    "title": "Тщетно, художник, ты мнишь, что творений своих ты создатель!"
  },
  {
    "poem_id": 16386,
    "author": "Толстой Алексей Константинович",
    "title": "Колокольчики мои…"
  },
  {
    "poem_id": 16387,
    "author": "Толстой Алексей Константинович",
    "title": "Ты знаешь край, где все обильем дышит…"
  },
  {
    "poem_id": 16388,
    "author": "Толстой Алексей Константинович",
    "title": "Б.М. Маркевичу («Ты прав; мой своенравный гений…»)"
  },
  {
    "poem_id": 16390,
    "author": "Толстой Алексей Константинович",
    "title": "К.К. Павловой («Прошу простить великодушно…»)"
  },
  {
    "poem_id": 16391,
    "author": "Толстой Алексей Константинович",
    "title": "М.Н. Лонгинову («Слава богу, я здоров…»)"
  },
  {
    "poem_id": 16392,
    "author": "Толстой Алексей Константинович",
    "title": "Об этом я терзаюся и плачу…"
  },
  {
    "poem_id": 16393,
    "author": "Толстой Алексей Константинович",
    "title": "Прогулка с подругой жизни"
  },
  {
    "poem_id": 16394,
    "author": "Толстой Алексей Константинович",
    "title": "Ты неведомое, незнамое…"
  },
  {
    "poem_id": 16395,
    "author": "Толстой Алексей Константинович",
    "title": "Грешница"
  },
  {
    "poem_id": 16396,
    "author": "Толстой Алексей Константинович",
    "title": "Бывают дни, когда злой дух меня тревожит…"
  },
  {
    "poem_id": 16397,
    "author": "Толстой Алексей Константинович",
    "title": "Ф.К. Мейендорфу («Барон, тебе, делившему…»)"
  },
  {
    "poem_id": 16398,
    "author": "Толстой Алексей Константинович",
    "title": "Вeсeнние чувства"
  },
  {
    "poem_id": 16399,
    "author": "Толстой Алексей Константинович",
    "title": "Я готов румянцем девичьим…"
  },
  {
    "poem_id": 16400,
    "author": "Толстой Алексей Константинович",
    "title": "Уж ты нива моя, нивушка…"
  },
  {
    "poem_id": 16401,
    "author": "Толстой Алексей Константинович",
    "title": "Смеркалось, жаркий день бледнел неуловимо…"
  },
  {
    "poem_id": 16402,
    "author": "Толстой Алексей Константинович",
    "title": "Милый друг, тебе не спится…"
  },
  {
    "poem_id": 16403,
    "author": "Толстой Алексей Константинович",
    "title": "Прозрачных облаков спокойное движенье…"
  },
  {
    "poem_id": 16404,
    "author": "Толстой Алексей Константинович",
    "title": "М.Н. Лонгинову («Очень, Лонгинов, мне жаль…»)"
  },
  {
    "poem_id": 16405,
    "author": "Толстой Алексей Константинович",
    "title": "Крымские очерки 9 («Приветствую тебя, опустошенный дом…»)"
  },
  {
    "poem_id": 16406,
    "author": "Толстой Алексей Константинович",
    "title": "В совести искал я долго обвиненья…"
  },
  {
    "poem_id": 16407,
    "author": "Толстой Алексей Константинович",
    "title": "Рондо"
  },
  {
    "poem_id": 16408,
    "author": "Толстой Алексей Константинович",
    "title": "Что за грустная обитель…"
  },
  {
    "poem_id": 16409,
    "author": "Толстой Алексей Константинович",
    "title": "Великодушие смягчает сердца («Вонзил кинжал убийца нечестивый…»)"
  },
  {
    "poem_id": 16410,
    "author": "Толстой Алексей Константинович",
    "title": "История государства Российского от Гостомысла до Тимашева"
  },
  {
    "poem_id": 16411,
    "author": "Толстой Алексей Константинович",
    "title": "Где гнутся над омутом лозы…"
  },
  {
    "poem_id": 16412,
    "author": "Толстой Алексей Константинович",
    "title": "Цыганские песни"
  },
  {
    "poem_id": 16413,
    "author": "Толстой Алексей Константинович",
    "title": "Змея, что по скалам влечешь свои извивы…"
  },
  {
    "poem_id": 16414,
    "author": "Толстой Алексей Константинович",
    "title": "Как-то Карп Семенович…"
  },
  {
    "poem_id": 16415,
    "author": "Толстой Алексей Константинович",
    "title": "Против течения"
  },
  {
    "poem_id": 16416,
    "author": "Толстой Алексей Константинович",
    "title": "Угораздило кофейник…"
  },
  {
    "poem_id": 16417,
    "author": "Толстой Алексей Константинович",
    "title": "О, если б ты могла хоть на единый миг…"
  },
  {
    "poem_id": 16418,
    "author": "Толстой Алексей Константинович",
    "title": "Источник за вишневым садом…"
  },
  {
    "poem_id": 16419,
    "author": "Толстой Алексей Константинович",
    "title": "Старицкий воевода"
  },
  {
    "poem_id": 16420,
    "author": "Толстой Алексей Константинович",
    "title": "Б.М. Маркевичу («Где нет толку никакого…»)"
  },
  {
    "poem_id": 16421,
    "author": "Толстой Алексей Константинович",
    "title": "Пусть тот, чья честь не без укора…"
  },
  {
    "poem_id": 16422,
    "author": "Толстой Алексей Константинович",
    "title": "Вот уж снег последний в поле тает…"
  },
  {
    "poem_id": 16423,
    "author": "Толстой Алексей Константинович",
    "title": "Двух станов не боец, но только гость случайный…"
  },
  {
    "poem_id": 16424,
    "author": "Толстой Алексей Константинович",
    "title": "Почему Александр I отказался от названия Великого"
  },
  {
    "poem_id": 16425,
    "author": "Толстой Алексей Константинович",
    "title": "Нет, уж не ведать мне, братцы, ни сна, ни покою!"
  },
  {
    "poem_id": 16426,
    "author": "Толстой Алексей Константинович",
    "title": "Ночь перед приступом"
  },
  {
    "poem_id": 16427,
    "author": "Толстой Алексей Константинович",
    "title": "Вихорь-конь"
  },
  {
    "poem_id": 16428,
    "author": "Толстой Алексей Константинович",
    "title": "Когда кругом безмолвен лес дремучий…"
  },
  {
    "poem_id": 16429,
    "author": "Толстой Алексей Константинович",
    "title": "Средь шумного бала, случайно…"
  },
  {
    "poem_id": 16430,
    "author": "Толстой Алексей Константинович",
    "title": "По гребле неровной и тряской…"
  },
  {
    "poem_id": 16431,
    "author": "Толстой Алексей Константинович",
    "title": "Деревцо мое миндальное…"
  },
  {
    "poem_id": 16432,
    "author": "Толстой Алексей Константинович",
    "title": "Ты меня поняла не вполне…"
  },
  {
    "poem_id": 16433,
    "author": "Толстой Алексей Константинович",
    "title": "Я вас узнал, святые убежденья…"
  },
  {
    "poem_id": 16434,
    "author": "Толстой Алексей Константинович",
    "title": "Я задремал, главу понуря…"
  },
  {
    "poem_id": 16435,
    "author": "Толстой Алексей Константинович",
    "title": "Вновь растворилась дверь на влажное крыльцо…"
  },
  {
    "poem_id": 16437,
    "author": "Толстой Алексей Константинович",
    "title": "С ружьем за плечами, один, при луне…"
  },
  {
    "poem_id": 16438,
    "author": "Толстой Алексей Константинович",
    "title": "Если б я был богом океана…"
  },
  {
    "poem_id": 16439,
    "author": "Толстой Алексей Константинович",
    "title": "Князь Михайло Репнин"
  },
  {
    "poem_id": 16440,
    "author": "Толстой Алексей Константинович",
    "title": "Честь вашего я круга…"
  },
  {
    "poem_id": 16441,
    "author": "Толстой Алексей Константинович",
    "title": "Во дни минувшие бывало…"
  },
  {
    "poem_id": 16442,
    "author": "Толстой Алексей Константинович",
    "title": "Баллада («Аскольд плывет, свой сняв шелом…»)"
  },
  {
    "poem_id": 16443,
    "author": "Толстой Алексей Константинович",
    "title": "Кабы знала я, кабы ведала…"
  },
  {
    "poem_id": 16444,
    "author": "Толстой Алексей Константинович",
    "title": "Ты знаешь, я люблю там, за лазурным сводом…"
  },
  {
    "poem_id": 16445,
    "author": "Толстой Алексей Константинович",
    "title": "У приказных ворот собирался народ…"
  },
  {
    "poem_id": 16446,
    "author": "Толстой Алексей Константинович",
    "title": "К твоим, царица, я ногам…"
  },
  {
    "poem_id": 16447,
    "author": "Толстой Алексей Константинович",
    "title": "Крымские очерки 2 («Клонит к лени полдень жгучий…»)"
  },
  {
    "poem_id": 16448,
    "author": "Толстой Алексей Константинович",
    "title": "Исполать тебе, жизнь — баба старая…"
  },
  {
    "poem_id": 16449,
    "author": "Толстой Алексей Константинович",
    "title": "Сердце, сильней разгораясь от году до году…"
  },
  {
    "poem_id": 16450,
    "author": "Толстой Алексей Константинович",
    "title": "Стасюлевич и Маркевич…"
  },
  {
    "poem_id": 16451,
    "author": "Толстой Алексей Константинович",
    "title": "Телескоп"
  },
  {
    "poem_id": 16452,
    "author": "Толстой Алексей Константинович",
    "title": "Ты почто, злая кручинушка…"
  },
  {
    "poem_id": 16454,
    "author": "Толстой Алексей Константинович",
    "title": "Ой, каб Волга-матушка да вспять побежала!.."
  },
  {
    "poem_id": 16455,
    "author": "Толстой Алексей Константинович",
    "title": "На тяге"
  },
  {
    "poem_id": 16456,
    "author": "Толстой Алексей Константинович",
    "title": "Аскольд, зовет тебя Мальвина…"
  },
  {
    "poem_id": 16457,
    "author": "Толстой Алексей Константинович",
    "title": "Меня, во мраке и в пыли…"
  },
  {
    "poem_id": 16458,
    "author": "Толстой Алексей Константинович",
    "title": "Край ты мой, родимый край…"
  },
  {
    "poem_id": 16459,
    "author": "Толстой Алексей Константинович",
    "title": "Как селянин, когда грозят…"
  },
  {
    "poem_id": 16460,
    "author": "Толстой Алексей Константинович",
    "title": "Про подвиг слышал я Кротонского бойца…"
  },
  {
    "poem_id": 16461,
    "author": "Толстой Алексей Константинович",
    "title": "О, не спеши туда, где жизнь светлей и чище…"
  },
  {
    "poem_id": 16462,
    "author": "Толстой Алексей Константинович",
    "title": "Что ни день, как поломя со влагой…"
  },
  {
    "poem_id": 16463,
    "author": "Толстой Алексей Константинович",
    "title": "Б.М. Маркевичу («В награду дружеских усилий…»)"
  },
  {
    "poem_id": 16464,
    "author": "Толстой Алексей Константинович",
    "title": "О, не страшись несбыточной измены…"
  },
  {
    "poem_id": 16465,
    "author": "Толстой Алексей Константинович",
    "title": "Усни, печальный друг, уже с грядущей тьмой…"
  },
  {
    "poem_id": 16467,
    "author": "Толстой Алексей Константинович",
    "title": "Он водил по струнам; упадали…"
  },
  {
    "poem_id": 16468,
    "author": "Толстой Алексей Константинович",
    "title": "Пустой дом"
  },
  {
    "poem_id": 16469,
    "author": "Толстой Алексей Константинович",
    "title": "Бегут разорванные тучи…"
  },
  {
    "poem_id": 16470,
    "author": "Толстой Алексей Константинович",
    "title": "Ой стоги, стоги…"
  },
  {
    "poem_id": 16471,
    "author": "Толстой Алексей Константинович",
    "title": "Князь Ростислав"
  },
  {
    "poem_id": 16472,
    "author": "Толстой Алексей Константинович",
    "title": "Курган"
  },
  {
    "poem_id": 16473,
    "author": "Толстой Алексей Константинович",
    "title": "Есть много звуков в сердца глубине…"
  },
  {
    "poem_id": 16474,
    "author": "Толстой Алексей Константинович",
    "title": "Песня о Каткове, о Черкасском, о Самарине, о Маркевиче и о арапах"
  },
  {
    "poem_id": 16475,
    "author": "Толстой Алексей Константинович",
    "title": "Не божиим громом горе ударило…"
  },
  {
    "poem_id": 16476,
    "author": "Толстой Алексей Константинович",
    "title": "Лишь только один я останусь с собою…"
  },
  {
    "poem_id": 16477,
    "author": "Толстой Алексей Константинович",
    "title": "Государь ты наш батюшка…"
  },
  {
    "poem_id": 16478,
    "author": "Толстой Алексей Константинович",
    "title": "В колокол, мирно дремавший, с налета тяжелая бомба…"
  },
  {
    "poem_id": 16479,
    "author": "Толстой Алексей Константинович",
    "title": "В монастыре пустынном близ Кордовы…"
  },
  {
    "poem_id": 16480,
    "author": "Толстой Алексей Константинович",
    "title": "Ругевит"
  },
  {
    "poem_id": 16481,
    "author": "Толстой Алексей Константинович",
    "title": "Вздымаются волны как горы…"
  },
  {
    "poem_id": 16482,
    "author": "Толстой Алексей Константинович",
    "title": "Молчите! — Мне не нужен розмарин!.."
  },
  {
    "poem_id": 16483,
    "author": "Толстой Алексей Константинович",
    "title": "Шумит на дворе непогода…"
  },
  {
    "poem_id": 16484,
    "author": "Толстой Алексей Константинович",
    "title": "Ты помнишь ли, Мария…"
  },
  {
    "poem_id": 16485,
    "author": "Толстой Алексей Константинович",
    "title": "Уж ты мать-тоска, горе-гореваньице!"
  },
  {
    "poem_id": 16487,
    "author": "Толстой Алексей Константинович",
    "title": "Ходит спесь, надуваючись…"
  },
  {
    "poem_id": 16488,
    "author": "Толстой Алексей Константинович",
    "title": "Когда природа вся трепещет и сияет…"
  },
  {
    "poem_id": 16489,
    "author": "Толстой Алексей Константинович",
    "title": "Нас не преследовала злоба…"
  },
  {
    "poem_id": 16490,
    "author": "Толстой Алексей Константинович",
    "title": "Желтобрюхого Гаврила…"
  },
  {
    "poem_id": 16491,
    "author": "Толстой Алексей Константинович",
    "title": "Боривой. Поморское сказание"
  },
  {
    "poem_id": 16493,
    "author": "Толстой Алексей Константинович",
    "title": "Ода на поимку Таирова"
  },
  {
    "poem_id": 16494,
    "author": "Толстой Алексей Константинович",
    "title": "Одарив весьма обильно…"
  },
  {
    "poem_id": 16495,
    "author": "Толстой Алексей Константинович",
    "title": "Сватовство"
  },
  {
    "poem_id": 16496,
    "author": "Толстой Алексей Константинович",
    "title": "Бегство Наполеона из России"
  },
  {
    "poem_id": 16497,
    "author": "Толстой Алексей Константинович",
    "title": "Сижу да гляжу я всe, братцы, вон в эту сторонку…"
  },
  {
    "poem_id": 16498,
    "author": "Толстой Алексей Константинович",
    "title": "Канут"
  },
  {
    "poem_id": 16499,
    "author": "Толстой Алексей Константинович",
    "title": "Чужое горе"
  },
  {
    "poem_id": 16500,
    "author": "Толстой Алексей Константинович",
    "title": "Прогулка под качелями"
  },
  {
    "poem_id": 16501,
    "author": "Толстой Алексей Константинович",
    "title": "Послание к М.Н. Лонгинову о дарвинисме"
  },
  {
    "poem_id": 16502,
    "author": "Толстой Алексей Константинович",
    "title": "Поток-богатырь"
  },
  {
    "poem_id": 16503,
    "author": "Толстой Алексей Константинович",
    "title": "Слеза дрожит в твоем ревнивом взоре…"
  },
  {
    "poem_id": 16504,
    "author": "Толстой Алексей Константинович",
    "title": "О, не пытайся дух унять тревожный…"
  },
  {
    "poem_id": 16505,
    "author": "Толстой Алексей Константинович",
    "title": "Стрелковые песни 1 («Слава на небе солнцу высокому!…»)"
  },
  {
    "poem_id": 16506,
    "author": "Толстой Алексей Константинович",
    "title": "Три побоища"
  },
  {
    "poem_id": 16507,
    "author": "Толстой Алексей Константинович",
    "title": "Грядой клубится белою…"
  },
  {
    "poem_id": 16508,
    "author": "Толстой Алексей Константинович",
    "title": "И.С. Аксакову («Судя меня довольно строго…»)"
  },
  {
    "poem_id": 16509,
    "author": "Толстой Алексей Константинович",
    "title": "Улыбка кроткая, в движенье каждом тихость…"
  },
  {
    "poem_id": 16510,
    "author": "Толстой Алексей Константинович",
    "title": "А.Н. Мальцевой («Пью ль мадеру, пью ли квас я…»)"
  },
  {
    "poem_id": 16511,
    "author": "Толстой Алексей Константинович",
    "title": "Алхимик"
  },
  {
    "poem_id": 16512,
    "author": "Толстой Алексей Константинович",
    "title": "Мне в душу, полную ничтожной суеты…"
  },
  {
    "poem_id": 16513,
    "author": "Толстой Алексей Константинович",
    "title": "Ушкуйник"
  },
  {
    "poem_id": 16514,
    "author": "Толстой Алексей Константинович",
    "title": "Графу Д.А. Толстому («Бисмарк, сидючи в Берлине…»)"
  },
  {
    "poem_id": 16515,
    "author": "Толстой Алексей Константинович",
    "title": "Не приставай ко мне, Борис Перовский…"
  },
  {
    "poem_id": 16516,
    "author": "Толстой Алексей Константинович",
    "title": "Не ветер, вея с высоты…"
  },
  {
    "poem_id": 16517,
    "author": "Толстой Алексей Константинович",
    "title": "Бор сосновый в стране одинокой стоит…"
  },
  {
    "poem_id": 16518,
    "author": "Толстой Алексей Константинович",
    "title": "Н.М. Жемчужникову («Горька нам, Николай…»)"
  },
  {
    "poem_id": 16519,
    "author": "Толстой Алексей Константинович",
    "title": "Рука Алкида тяжела…"
  },
  {
    "poem_id": 16520,
    "author": "Толстой Алексей Константинович",
    "title": "Замолкнул гром, шуметь гроза устала…"
  },
  {
    "poem_id": 16521,
    "author": "Толстой Алексей Константинович",
    "title": "На нивы желтые нисходит тишина…"
  },
  {
    "poem_id": 16522,
    "author": "Толстой Алексей Константинович",
    "title": "О, будь же мене голосист…"
  },
  {
    "poem_id": 16523,
    "author": "Толстой Алексей Константинович",
    "title": "Пусто в покое моем. Один я сижу у камина…"
  },
  {
    "poem_id": 16524,
    "author": "Толстой Алексей Константинович",
    "title": "Горними тихо летела душа небесами…"
  },
  {
    "poem_id": 16525,
    "author": "Толстой Алексей Константинович",
    "title": "Растянулся на просторе…"
  },
  {
    "poem_id": 16526,
    "author": "Толстой Алексей Константинович",
    "title": "Не верь мне, друг, когда, в избытке горя…"
  },
  {
    "poem_id": 16527,
    "author": "Толстой Алексей Константинович",
    "title": "В альбом («Стрелок, на той поляне…»)"
  },
  {
    "poem_id": 16528,
    "author": "Толстой Алексей Константинович",
    "title": "Что ты голову склонила.."
  },
  {
    "poem_id": 16529,
    "author": "Толстой Алексей Константинович",
    "title": "Медицинские стихотворения"
  },
  {
    "poem_id": 16530,
    "author": "Толстой Алексей Константинович",
    "title": "Не брани меня, мой друг…"
  },
  {
    "poem_id": 16531,
    "author": "Толстой Алексей Константинович",
    "title": "Змей Тугарин"
  },
  {
    "poem_id": 16532,
    "author": "Толстой Алексей Константинович",
    "title": "O друг, ты жизнь влачишь, без пользы увядая…"
  },
  {
    "poem_id": 16533,
    "author": "Толстой Алексей Константинович",
    "title": "Богатырь"
  },
  {
    "poem_id": 16534,
    "author": "Константин Михайлович Фофанов",
    "title": "Кукушка"
  },
  {
    "poem_id": 16535,
    "author": "Константин Михайлович Фофанов",
    "title": "Сегодня в ночь весна-колдунья…"
  },
  {
    "poem_id": 16536,
    "author": "Константин Михайлович Фофанов",
    "title": "Снегурка"
  },
  {
    "poem_id": 16537,
    "author": "Константин Михайлович Фофанов",
    "title": "Два гения"
  },
  {
    "poem_id": 16538,
    "author": "Константин Михайлович Фофанов",
    "title": "Голод"
  },
  {
    "poem_id": 16539,
    "author": "Константин Михайлович Фофанов",
    "title": "Как стучит уныло маятник…"
  },
  {
    "poem_id": 16540,
    "author": "Константин Михайлович Фофанов",
    "title": "Столица бредила в чаду своей тоски…"
  },
  {
    "poem_id": 16541,
    "author": "Константин Михайлович Фофанов",
    "title": "В альбом В. В. Бертенсона"
  },
  {
    "poem_id": 16542,
    "author": "Константин Михайлович Фофанов",
    "title": "Прекрасна эта ночь с ее красой печальной…"
  },
  {
    "poem_id": 16543,
    "author": "Константин Михайлович Фофанов",
    "title": "Шумят леса тенистые…"
  },
  {
    "poem_id": 16544,
    "author": "Константин Михайлович Фофанов",
    "title": "Из старого альбома"
  },
  {
    "poem_id": 16545,
    "author": "Константин Михайлович Фофанов",
    "title": "Прекрасна ты, осенняя пора…"
  },
  {
    "poem_id": 16546,
    "author": "Константин Михайлович Фофанов",
    "title": "Скошенные травы"
  },
  {
    "poem_id": 16547,
    "author": "Константин Михайлович Фофанов",
    "title": "Май"
  },
  {
    "poem_id": 16548,
    "author": "Константин Михайлович Фофанов",
    "title": "Желтыми листьями дети играли…"
  },
  {
    "poem_id": 16549,
    "author": "Константин Михайлович Фофанов",
    "title": "Дума в Царском селе"
  },
  {
    "poem_id": 16550,
    "author": "Константин Михайлович Фофанов",
    "title": "Осень"
  },
  {
    "poem_id": 16551,
    "author": "Константин Михайлович Фофанов",
    "title": "Старый сад"
  },
  {
    "poem_id": 16552,
    "author": "Константин Михайлович Фофанов",
    "title": "Аллея осенью"
  },
  {
    "poem_id": 16553,
    "author": "Константин Михайлович Фофанов",
    "title": "Не правда ль, всё дышало прозой…"
  },
  {
    "poem_id": 16554,
    "author": "Константин Михайлович Фофанов",
    "title": "Исполнен горького упрека…"
  },
  {
    "poem_id": 16555,
    "author": "Константин Михайлович Фофанов",
    "title": "Прошла любовь, прошла гроза…"
  },
  {
    "poem_id": 16556,
    "author": "Константин Михайлович Фофанов",
    "title": "Под напев молитв пасхальных…"
  },
  {
    "poem_id": 16557,
    "author": "Константин Михайлович Фофанов",
    "title": "Как воздух свеж, как липы ярко…"
  },
  {
    "poem_id": 16558,
    "author": "Константин Михайлович Фофанов",
    "title": "Журавли"
  },
  {
    "poem_id": 16559,
    "author": "Константин Михайлович Фофанов",
    "title": "Осенью"
  },
  {
    "poem_id": 16560,
    "author": "Константин Михайлович Фофанов",
    "title": "С тоской в груди и гневом смутным…"
  },
  {
    "poem_id": 16561,
    "author": "Константин Михайлович Фофанов",
    "title": "Печально верба наклоняла…"
  },
  {
    "poem_id": 16562,
    "author": "Константин Михайлович Фофанов",
    "title": "Сумерки бледные, сумерки мутные…"
  },
  {
    "poem_id": 16563,
    "author": "Константин Михайлович Фофанов",
    "title": "Мой друг, мой нежный друг, люблю тебя — зову…"
  },
  {
    "poem_id": 16564,
    "author": "Константин Михайлович Фофанов",
    "title": "Догорает мой светильник…"
  },
  {
    "poem_id": 16565,
    "author": "Константин Михайлович Фофанов",
    "title": "Как будто раннею весною…"
  },
  {
    "poem_id": 16566,
    "author": "Константин Михайлович Фофанов",
    "title": "Лунная тихая ночь…"
  },
  {
    "poem_id": 16567,
    "author": "Константин Михайлович Фофанов",
    "title": "Умолк весенний гром. Всё блещет и поет…."
  },
  {
    "poem_id": 16568,
    "author": "Константин Михайлович Фофанов",
    "title": "Дрожащий блеск звезды вечерней…"
  },
  {
    "poem_id": 16569,
    "author": "Константин Михайлович Фофанов",
    "title": "Как быстро год прошел! Как медленные дни…"
  },
  {
    "poem_id": 16570,
    "author": "Константин Михайлович Фофанов",
    "title": "У потока"
  },
  {
    "poem_id": 16571,
    "author": "Константин Михайлович Фофанов",
    "title": "Вечернее небо, лазурные воды…"
  },
  {
    "poem_id": 16572,
    "author": "Константин Михайлович Фофанов",
    "title": "Одуванчик"
  },
  {
    "poem_id": 16573,
    "author": "Константин Михайлович Фофанов",
    "title": "Небо и море"
  },
  {
    "poem_id": 16574,
    "author": "Константин Михайлович Фофанов",
    "title": "После грозы"
  },
  {
    "poem_id": 16575,
    "author": "Константин Михайлович Фофанов",
    "title": "Сколько жизни, сколько блеску…"
  },
  {
    "poem_id": 16576,
    "author": "Константин Михайлович Фофанов",
    "title": "На деревенском кладбище"
  },
  {
    "poem_id": 16577,
    "author": "Константин Михайлович Фофанов",
    "title": "В роще"
  },
  {
    "poem_id": 16578,
    "author": "Константин Михайлович Фофанов",
    "title": "Пел соловей, цветы благоухали…"
  },
  {
    "poem_id": 16579,
    "author": "Константин Михайлович Фофанов",
    "title": "Уснули и травы и волны…"
  },
  {
    "poem_id": 16580,
    "author": "Константин Михайлович Фофанов",
    "title": "У печки"
  },
  {
    "poem_id": 16581,
    "author": "Константин Михайлович Фофанов",
    "title": "Шиллер. Юноша у ручья"
  },
  {
    "poem_id": 16582,
    "author": "Константин Михайлович Фофанов",
    "title": "Зашумел, закачался взволнованный сад…"
  },
  {
    "poem_id": 16583,
    "author": "Константин Михайлович Фофанов",
    "title": "Была ль то песнь, рожденная мечтою…"
  },
  {
    "poem_id": 16584,
    "author": "Константин Михайлович Фофанов",
    "title": "На волне колокольного звона…"
  },
  {
    "poem_id": 16585,
    "author": "Константин Михайлович Фофанов",
    "title": "Не бойся сумрака могилы…"
  },
  {
    "poem_id": 16586,
    "author": "Константин Михайлович Фофанов",
    "title": "Мы при свечах болтали долго…"
  },
  {
    "poem_id": 16587,
    "author": "Константин Михайлович Фофанов",
    "title": "Печальный румянец заката…"
  },
  {
    "poem_id": 16588,
    "author": "Константин Михайлович Фофанов",
    "title": "Вильям Шекспир сонет 109 (О нет, не говори, что сердцем пред тобою…)"
  },
  {
    "poem_id": 16589,
    "author": "Константин Михайлович Фофанов",
    "title": "А. А. Фету (Есть в природе бесконечной…)"
  },
  {
    "poem_id": 16590,
    "author": "Константин Михайлович Фофанов",
    "title": "Что ты сказала, я не расслышал…"
  },
  {
    "poem_id": 16591,
    "author": "Константин Михайлович Фофанов",
    "title": "Вильям Шекспир сонет 107 (Ни собственный мой страх…)"
  },
  {
    "poem_id": 16592,
    "author": "Константин Михайлович Фофанов",
    "title": "Мир поэта"
  },
  {
    "poem_id": 16593,
    "author": "Константин Михайлович Фофанов",
    "title": "Звезды ясные, звезды прекрасные…"
  },
  {
    "poem_id": 16594,
    "author": "Константин Михайлович Фофанов",
    "title": "Луч"
  },
  {
    "poem_id": 16595,
    "author": "Константин Михайлович Фофанов",
    "title": "Всё то же"
  },
  {
    "poem_id": 16596,
    "author": "Константин Михайлович Фофанов",
    "title": "Еще повсюду в спящем парке…"
  },
  {
    "poem_id": 16597,
    "author": "Константин Михайлович Фофанов",
    "title": "Чем смертоносней влага в чаше…"
  },
  {
    "poem_id": 16598,
    "author": "Константин Михайлович Фофанов",
    "title": "Волки. Рождественский рассказ"
  },
  {
    "poem_id": 16599,
    "author": "Константин Михайлович Фофанов",
    "title": "Листья"
  },
  {
    "poem_id": 16600,
    "author": "Константин Михайлович Фофанов",
    "title": "Наш домовой"
  },
  {
    "poem_id": 16601,
    "author": "Константин Михайлович Фофанов",
    "title": "Стало скучно тебе…"
  },
  {
    "poem_id": 16602,
    "author": "Константин Михайлович Фофанов",
    "title": "Мысли мятежные"
  },
  {
    "poem_id": 16603,
    "author": "Константин Михайлович Фофанов",
    "title": "Под музыку осеннего дождя"
  },
  {
    "poem_id": 16604,
    "author": "Константин Михайлович Фофанов",
    "title": "Стансы"
  },
  {
    "poem_id": 16605,
    "author": "Константин Михайлович Фофанов",
    "title": "Ещё те звезды не погасли…"
  },
  {
