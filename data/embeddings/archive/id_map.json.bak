  {
    "poem_id": 10749,
    "author": "Ахматова Анна Андреевна",
    "title": "О, как меня любили ваши деды…"
  },
  {
    "poem_id": 10750,
    "author": "Ахматова Анна Андреевна",
    "title": "Страх, во тьме перебирая вещи…"
  },
  {
    "poem_id": 10751,
    "author": "Ахматова Анна Андреевна",
    "title": "У меня есть улыбка одна…"
  },
  {
    "poem_id": 10752,
    "author": "Ахматова Анна Андреевна",
    "title": "Я и плакала и каялась…"
  },
  {
    "poem_id": 10753,
    "author": "Ахматова Анна Андреевна",
    "title": "Музе (Муза-сестра заглянула в лицо…)"
  },
  {
    "poem_id": 10754,
    "author": "Ахматова Анна Андреевна",
    "title": "На шее мелких четок ряд…"
  },
  {
    "poem_id": 10755,
    "author": "Ахматова Анна Андреевна",
    "title": "Третью весну встречаю вдали…"
  },
  {
    "poem_id": 10756,
    "author": "Ахматова Анна Андреевна",
    "title": "Борис Пастернак (поэт)"
  },
  {
    "poem_id": 10757,
    "author": "Ахматова Анна Андреевна",
    "title": "Июль 1914"
  },
  {
    "poem_id": 10758,
    "author": "Ахматова Анна Андреевна",
    "title": "Поэт"
  },
  {
    "poem_id": 10759,
    "author": "Ахматова Анна Андреевна",
    "title": "Отрывок"
  },
  {
    "poem_id": 10760,
    "author": "Ахматова Анна Андреевна",
    "title": "Это просто, это ясно…"
  },
  {
    "poem_id": 10761,
    "author": "Ахматова Анна Андреевна",
    "title": "Песня последней встречи"
  },
  {
    "poem_id": 10762,
    "author": "Ахматова Анна Андреевна",
    "title": "Реквием"
  },
  {
    "poem_id": 10763,
    "author": "Ахматова Анна Андреевна",
    "title": "Углем наметил на левом боку…"
  },
  {
    "poem_id": 10764,
    "author": "Ахматова Анна Андреевна",
    "title": "Лондонцам"
  },
  {
    "poem_id": 10765,
    "author": "Ахматова Анна Андреевна",
    "title": "Не в лесу мы, довольно аукать…"
  },
  {
    "poem_id": 10766,
    "author": "Ахматова Анна Андреевна",
    "title": "Высокие своды костела…"
  },
  {
    "poem_id": 10767,
    "author": "Ахматова Анна Андреевна",
    "title": "Дал Ты мне молодость трудную…"
  },
  {
    "poem_id": 10768,
    "author": "Ахматова Анна Андреевна",
    "title": "Где, высокая, твой цыганенок…"
  },
  {
    "poem_id": 10769,
    "author": "Ахматова Анна Андреевна",
    "title": "А я говорю, вероятно, за многих…"
  },
  {
    "poem_id": 10770,
    "author": "Ахматова Анна Андреевна",
    "title": "Стихи о Петербурге"
  },
  {
    "poem_id": 10771,
    "author": "Ахматова Анна Андреевна",
    "title": "Выбрала сама я долю…"
  },
  {
    "poem_id": 10772,
    "author": "Ахматова Анна Андреевна",
    "title": "Не тайны и не печали…"
  },
  {
    "poem_id": 10773,
    "author": "Ахматова Анна Андреевна",
    "title": "Особенных претензий не имею…"
  },
  {
    "poem_id": 10774,
    "author": "Ахматова Анна Андреевна",
    "title": "И как всегда бывает в дни разрыва…"
  },
  {
    "poem_id": 10775,
    "author": "Ахматова Анна Андреевна",
    "title": "Небо мелкий дождик сеет…"
  },
  {
    "poem_id": 10776,
    "author": "Ахматова Анна Андреевна",
    "title": "Тень"
  },
  {
    "poem_id": 10777,
    "author": "Ахматова Анна Андреевна",
    "title": "И вот, наперекор тому…"
  },
  {
    "poem_id": 10778,
    "author": "Ахматова Анна Андреевна",
    "title": "Бессмертник сух и розов. Облака…"
  },
  {
    "poem_id": 10779,
    "author": "Ахматова Анна Андреевна",
    "title": "Наследница"
  },
  {
    "poem_id": 10780,
    "author": "Ахматова Анна Андреевна",
    "title": "И снова осень валит Тамерланом…"
  },
  {
    "poem_id": 10781,
    "author": "Ахматова Анна Андреевна",
    "title": "О своем я уже не заплачу…"
  },
  {
    "poem_id": 10782,
    "author": "Ахматова Анна Андреевна",
    "title": "Проводила друга до передней…"
  },
  {
    "poem_id": 10783,
    "author": "Ахматова Анна Андреевна",
    "title": "Надпись на книге"
  },
  {
    "poem_id": 10784,
    "author": "Ахматова Анна Андреевна",
    "title": "Я горькая и старая. Морщины…"
  },
  {
    "poem_id": 10785,
    "author": "Ахматова Анна Андреевна",
    "title": "И мнится — голос человека…"
  },
  {
    "poem_id": 10786,
    "author": "Ахматова Анна Андреевна",
    "title": "Пива светлого наварено…"
  },
  {
    "poem_id": 10787,
    "author": "Ахматова Анна Андреевна",
    "title": "А ты теперь тяжелый и унылый…"
  },
  {
    "poem_id": 10788,
    "author": "Ахматова Анна Андреевна",
    "title": "Протертый коврик под иконой…"
  },
  {
    "poem_id": 10789,
    "author": "Ахматова Анна Андреевна",
    "title": "Глаз не свожу с горизонта…"
  },
  {
    "poem_id": 10790,
    "author": "Ахматова Анна Андреевна",
    "title": "Памяти друга"
  },
  {
    "poem_id": 10791,
    "author": "Ахматова Анна Андреевна",
    "title": "И вот одна осталась я…"
  },
  {
    "poem_id": 10792,
    "author": "Ахматова Анна Андреевна",
    "title": "Нам встречи нет. Мы в разных станах…"
  },
  {
    "poem_id": 10793,
    "author": "Ахматова Анна Андреевна",
    "title": "Данте"
  },
  {
    "poem_id": 10794,
    "author": "Ахматова Анна Андреевна",
    "title": "Ангел, три года хранивший меня…"
  },
  {
    "poem_id": 10795,
    "author": "Ахматова Анна Андреевна",
    "title": "8 ноября 1913 года"
  },
  {
    "poem_id": 10796,
    "author": "Ахматова Анна Андреевна",
    "title": "В тот давний год, когда зажглась любовь…"
  },
  {
    "poem_id": 10797,
    "author": "Ахматова Анна Андреевна",
    "title": "Учитель"
  },
  {
    "poem_id": 10798,
    "author": "Ахматова Анна Андреевна",
    "title": "Эпиграмма"
  },
  {
    "poem_id": 10799,
    "author": "Ахматова Анна Андреевна",
    "title": "Нам свежесть слов и чувства простоту…"
  },
  {
    "poem_id": 10800,
    "author": "Ахматова Анна Андреевна",
    "title": "Вот она, плодоносная осень…"
  },
  {
    "poem_id": 10801,
    "author": "Ахматова Анна Андреевна",
    "title": "Помолись о нищей, о потерянной…"
  },
  {
    "poem_id": 10802,
    "author": "Ахматова Анна Андреевна",
    "title": "Когда лежит луна ломтем чарджуйской дыни…"
  },
  {
    "poem_id": 10803,
    "author": "Ахматова Анна Андреевна",
    "title": "Подражание И. Ф. Анненскому (И с тобой, моей первой причудой…)"
  },
  {
    "poem_id": 10804,
    "author": "Ахматова Анна Андреевна",
    "title": "Соблазна не было. Соблазн в тиши живет…"
  },
  {
    "poem_id": 10805,
    "author": "Ахматова Анна Андреевна",
    "title": "Со дня Купальницы-Аграфены…"
  },
  {
    "poem_id": 10806,
    "author": "Ахматова Анна Андреевна",
    "title": "Перед весной бывают дни такие…"
  },
  {
    "poem_id": 10807,
    "author": "Ахматова Анна Андреевна",
    "title": "В том доме было очень страшно жить…"
  },
  {
    "poem_id": 10808,
    "author": "Ахматова Анна Андреевна",
    "title": "Когда человек умирает…"
  },
  {
    "poem_id": 10809,
    "author": "Ахматова Анна Андреевна",
    "title": "Белый дом"
  },
  {
    "poem_id": 10810,
    "author": "Ахматова Анна Андреевна",
    "title": "Кому и когда говорила…"
  },
  {
    "poem_id": 10811,
    "author": "Ахматова Анна Андреевна",
    "title": "Непогребенных всех – я хоронила их…"
  },
  {
    "poem_id": 10812,
    "author": "Ахматова Анна Андреевна",
    "title": "Буду тихо на погосте…"
  },
  {
    "poem_id": 10813,
    "author": "Ахматова Анна Андреевна",
    "title": "И когда друг друга проклинали…"
  },
  {
    "poem_id": 10814,
    "author": "Ахматова Анна Андреевна",
    "title": "Немного географии"
  },
  {
    "poem_id": 10815,
    "author": "Ахматова Анна Андреевна",
    "title": "Зачем вы отравили воду…"
  },
  {
    "poem_id": 10816,
    "author": "Ахматова Анна Андреевна",
    "title": "В последний раз мы встретились тогда…"
  },
  {
    "poem_id": 10817,
    "author": "Ахматова Анна Андреевна",
    "title": "Творчество"
  },
  {
    "poem_id": 10818,
    "author": "Ахматова Анна Андреевна",
    "title": "Как невеста, получаю…"
  },
  {
    "poem_id": 10819,
    "author": "Ахматова Анна Андреевна",
    "title": "Как у облака на краю…"
  },
  {
    "poem_id": 10821,
    "author": "Ахматова Анна Андреевна",
    "title": "Город сгинул, последнего дома…"
  },
  {
    "poem_id": 10822,
    "author": "Ахматова Анна Андреевна",
    "title": "В последний год, когда столица наша…"
  },
  {
    "poem_id": 10823,
    "author": "Ахматова Анна Андреевна",
    "title": "Здесь всё то же, то же, что и прежде…"
  },
  {
    "poem_id": 10824,
    "author": "Ахматова Анна Андреевна",
    "title": "Царскосельская статуя"
  },
  {
    "poem_id": 10825,
    "author": "Ахматова Анна Андреевна",
    "title": "Лучше б мне частушки задорно выкликать…"
  },
  {
    "poem_id": 10826,
    "author": "Ахматова Анна Андреевна",
    "title": "О тебе вспоминаю я редко…"
  },
  {
    "poem_id": 10827,
    "author": "Ахматова Анна Андреевна",
    "title": "От меня, как от той графини…"
  },
  {
    "poem_id": 10828,
    "author": "Ахматова Анна Андреевна",
    "title": "Пусть даже вылета мне нет…"
  },
  {
    "poem_id": 10829,
    "author": "Ахматова Анна Андреевна",
    "title": "На столике чай, печения сдобные…"
  },
  {
    "poem_id": 10830,
    "author": "Ахматова Анна Андреевна",
    "title": "Побег"
  },
  {
    "poem_id": 10831,
    "author": "Ахматова Анна Андреевна",
    "title": "Я знаю, ты моя награда…"
  },
  {
    "poem_id": 10832,
    "author": "Ахматова Анна Андреевна",
    "title": "Тот город, мной любимый с детства…"
  },
  {
    "poem_id": 10833,
    "author": "Ахматова Анна Андреевна",
    "title": "Меня покинул в новолунье…"
  },
  {
    "poem_id": 10834,
    "author": "Ахматова Анна Андреевна",
    "title": "В заветных ладанках не носим на груди…"
  },
  {
    "poem_id": 10835,
    "author": "Ахматова Анна Андреевна",
    "title": "От тебя я сердце скрыла…"
  },
  {
    "poem_id": 10836,
    "author": "Ахматова Анна Андреевна",
    "title": "Земной отрадой сердца не томи…"
  },
  {
    "poem_id": 10837,
    "author": "Ахматова Анна Андреевна",
    "title": "В ремешках пенал и книги были…"
  },
  {
    "poem_id": 10838,
    "author": "Ахматова Анна Андреевна",
    "title": "Будешь жить, не зная лиха…"
  },
  {
    "poem_id": 10839,
    "author": "Ахматова Анна Андреевна",
    "title": "Приморский сонет"
  },
  {
    "poem_id": 10840,
    "author": "Ахматова Анна Андреевна",
    "title": "Новогодняя баллада"
  },
  {
    "poem_id": 10841,
    "author": "Ахматова Анна Андреевна",
    "title": "Маяковский в 1913 году"
  },
  {
    "poem_id": 10842,
    "author": "Ахматова Анна Андреевна",
    "title": "Мне не надо счастья малого…"
  },
  {
    "poem_id": 10843,
    "author": "Ахматова Анна Андреевна",
    "title": "И в памяти черной, пошарив, найдешь…"
  },
  {
    "poem_id": 10844,
    "author": "Ахматова Анна Андреевна",
    "title": "Подвал памяти"
  },
  {
    "poem_id": 10845,
    "author": "Ахматова Анна Андреевна",
    "title": "Из памяти твоей я выну этот день…"
  },
  {
    "poem_id": 10846,
    "author": "Ахматова Анна Андреевна",
    "title": "Просыпаться на рассвете…"
  },
  {
    "poem_id": 10847,
    "author": "Ахматова Анна Андреевна",
    "title": "Одни глядятся в ласковые взоры…"
  },
  {
    "poem_id": 10848,
    "author": "Ахматова Анна Андреевна",
    "title": "Я над ними склонюсь, как над чашей…"
  },
  {
    "poem_id": 10849,
    "author": "Ахматова Анна Андреевна",
    "title": "Был он ревнивым, тревожным и нежным…"
  },
  {
    "poem_id": 10850,
    "author": "Ахматова Анна Андреевна",
    "title": "По твердому гребню сугроба…"
  },
  {
    "poem_id": 10851,
    "author": "Ахматова Анна Андреевна",
    "title": "Как в трапезной – скамейки, стол, окно…"
  },
  {
    "poem_id": 10852,
    "author": "Ахматова Анна Андреевна",
    "title": "Меня, как реку…"
  },
  {
    "poem_id": 10853,
    "author": "Ахматова Анна Андреевна",
    "title": "Когда о горькой гибели моей…"
  },
  {
    "poem_id": 10854,
    "author": "Ахматова Анна Андреевна",
    "title": "Вечером"
  },
  {
    "poem_id": 10855,
    "author": "Ахматова Анна Андреевна",
    "title": "Сердце к сердцу не приковано…"
  },
  {
    "poem_id": 10856,
    "author": "Ахматова Анна Андреевна",
    "title": "Тот август, как желтое пламя…"
  },
  {
    "poem_id": 10857,
    "author": "Ахматова Анна Андреевна",
    "title": "Дверь полуоткрыта…"
  },
  {
    "poem_id": 10858,
    "author": "Ахматова Анна Андреевна",
    "title": "Призрак"
  },
  {
    "poem_id": 10859,
    "author": "Ахматова Анна Андреевна",
    "title": "Из завещания Васильки"
  },
  {
    "poem_id": 10860,
    "author": "Ахматова Анна Андреевна",
    "title": "Третий Зачатьевский"
  },
  {
    "poem_id": 10861,
    "author": "Ахматова Анна Андреевна",
    "title": "Вместо мудрости – опытность, пресное…"
  },
  {
    "poem_id": 10862,
    "author": "Ахматова Анна Андреевна",
    "title": "Не недели, не месяцы – годы…"
  },
  {
    "poem_id": 10863,
    "author": "Ахматова Анна Андреевна",
    "title": "Мне с тобою пьяным весело…"
  },
  {
    "poem_id": 10864,
    "author": "Ахматова Анна Андреевна",
    "title": "И мальчик, что играет на волынке…"
  },
  {
    "poem_id": 10865,
    "author": "Ахматова Анна Андреевна",
    "title": "Здравствуй, Питер! Плохо, старый…"
  },
  {
    "poem_id": 10866,
    "author": "Ахматова Анна Андреевна",
    "title": "Они летят, они еще в дороге…"
  },
  {
    "poem_id": 10867,
    "author": "Ахматова Анна Андреевна",
    "title": "Я знаю, с места не сдвинуться…"
  },
  {
    "poem_id": 10868,
    "author": "Ахматова Анна Андреевна",
    "title": "Заплаканная осень, как вдова…"
  },
  {
    "poem_id": 10869,
    "author": "Ахматова Анна Андреевна",
    "title": "Поздний ответ"
  },
  {
    "poem_id": 10870,
    "author": "Ахматова Анна Андреевна",
    "title": "Там тень моя осталась и тоскует…"
  },
  {
    "poem_id": 10871,
    "author": "Ахматова Анна Андреевна",
    "title": "Он любил…"
  },
  {
    "poem_id": 10872,
    "author": "Ахматова Анна Андреевна",
    "title": "Каждый день по-новому тревожен…"
  },
  {
    "poem_id": 10873,
    "author": "Ахматова Анна Андреевна",
    "title": "Постучись кулачком – я открою…"
  },
  {
    "poem_id": 10874,
    "author": "Ахматова Анна Андреевна",
    "title": "Как ты можешь смотреть на Неву…"
  },
  {
    "poem_id": 10875,
    "author": "Ахматова Анна Андреевна",
    "title": "Я с тобой не стану пить вино…"
  },
  {
    "poem_id": 10876,
    "author": "Ахматова Анна Андреевна",
    "title": "Какая есть. Желаю вам другую…"
  },
  {
    "poem_id": 10877,
    "author": "Ахматова Анна Андреевна",
    "title": "Потускнел на небе синий лак…"
  },
  {
    "poem_id": 10878,
    "author": "Ахматова Анна Андреевна",
    "title": "Будем вместе, милый, вместе…"
  },
  {
    "poem_id": 10879,
    "author": "Ахматова Анна Андреевна",
    "title": "Памяти Пильняка"
  },
  {
    "poem_id": 10880,
    "author": "Ахматова Анна Андреевна",
    "title": "А! Это снова ты. Не отроком влюбленным…"
  },
  {
    "poem_id": 10881,
    "author": "Ахматова Анна Андреевна",
    "title": "Я пришла к поэту в гости…"
  },
  {
    "poem_id": 10882,
    "author": "Ахматова Анна Андреевна",
    "title": "Не будем пить из одного стакана…"
  },
  {
    "poem_id": 10883,
    "author": "Ахматова Анна Андреевна",
    "title": "Бежецк"
  },
  {
    "poem_id": 10884,
    "author": "Ахматова Анна Андреевна",
    "title": "И слава лебедью плыла…"
  },
  {
    "poem_id": 10885,
    "author": "Ахматова Анна Андреевна",
    "title": "Сказал, что у меня соперниц нет…"
  },
  {
    "poem_id": 10886,
    "author": "Ахматова Анна Андреевна",
    "title": "Рисунок на книге стихов"
  },
  {
    "poem_id": 10887,
    "author": "Ахматова Анна Андреевна",
    "title": "О, это был прохладный день…"
  },
  {
    "poem_id": 10888,
    "author": "Ахматова Анна Андреевна",
    "title": "Дьявол не выдал. Мне всё удалось…"
  },
  {
    "poem_id": 10889,
    "author": "Ахматова Анна Андреевна",
    "title": "И было сердцу ничего не надо…"
  },
  {
    "poem_id": 10890,
    "author": "Ахматова Анна Андреевна",
    "title": "Мужество"
  },
  {
    "poem_id": 10891,
    "author": "Ахматова Анна Андреевна",
    "title": "Словно дочка слепого Эдипа…"
  },
  {
    "poem_id": 10892,
    "author": "Ахматова Анна Андреевна",
    "title": "Настоящую нежность не спутаешь…"
  },
  {
    "poem_id": 10893,
    "author": "Ахматова Анна Андреевна",
    "title": "Сжала руки под темной вуалью…"
  },
  {
    "poem_id": 10894,
    "author": "Ахматова Анна Андреевна",
    "title": "Сонет"
  },
  {
    "poem_id": 10895,
    "author": "Ахматова Анна Андреевна",
    "title": "На пороге белом рая…"
  },
  {
    "poem_id": 10896,
    "author": "Ахматова Анна Андреевна",
    "title": "А, ты думал – я тоже такая…"
  },
  {
    "poem_id": 10897,
    "author": "Ахматова Анна Андреевна",
    "title": "Как люблю, как любила глядеть я…"
  },
  {
    "poem_id": 10898,
    "author": "Ахматова Анна Андреевна",
    "title": "И в Киевском храме Премудрости Бога…"
  },
  {
    "poem_id": 10899,
    "author": "Ахматова Анна Андреевна",
    "title": "Ива"
  },
  {
    "poem_id": 10900,
    "author": "Ахматова Анна Андреевна",
    "title": "Вновь подарен мне дремотой…"
  },
  {
    "poem_id": 10901,
    "author": "Ахматова Анна Андреевна",
    "title": "Истлевают звуки в эфире…"
  },
  {
    "poem_id": 10902,
    "author": "Ахматова Анна Андреевна",
    "title": "Отстояли нас наши мальчишки…"
  },
  {
    "poem_id": 10903,
    "author": "Ахматова Анна Андреевна",
    "title": "Был блаженной моей колыбелью…"
  },
  {
    "poem_id": 10904,
    "author": "Ахматова Анна Андреевна",
    "title": "Птицы смерти в зените стоят…"
  },
  {
    "poem_id": 10905,
    "author": "Ахматова Анна Андреевна",
    "title": "Годовщину последнюю празднуй…"
  },
  {
    "poem_id": 10906,
    "author": "Ахматова Анна Андреевна",
    "title": "По неделе ни слова ни с кем не скажу…"
  },
  {
    "poem_id": 10907,
    "author": "Ахматова Анна Андреевна",
    "title": "И сердце то уже не отзовется…"
  },
  {
    "poem_id": 10908,
    "author": "Ахматова Анна Андреевна",
    "title": "Один идет прямым путем…"
  },
  {
    "poem_id": 10909,
    "author": "Ахматова Анна Андреевна",
    "title": "Слаб голос мой, но воля не слабеет…"
  },
  {
    "poem_id": 10910,
    "author": "Ахматова Анна Андреевна",
    "title": "…А там мой мраморный двойник…"
  },
  {
    "poem_id": 10911,
    "author": "Ахматова Анна Андреевна",
    "title": "Ты мне не обещан ни жизнью, ни Богом…"
  },
  {
    "poem_id": 10912,
    "author": "Ахматова Анна Андреевна",
    "title": "Как будто страшной песенки…"
  },
  {
    "poem_id": 10913,
    "author": "Ахматова Анна Андреевна",
    "title": "Я с тобой, мой ангел, не лукавил…"
  },
  {
    "poem_id": 10914,
    "author": "Ахматова Анна Андреевна",
    "title": "За озером луна остановилась…"
  },
  {
    "poem_id": 10915,
    "author": "Ахматова Анна Андреевна",
    "title": "Причитание"
  },
  {
    "poem_id": 10916,
    "author": "Ахматова Анна Андреевна",
    "title": "Шелестит о прошлом старый дуб…"
  },
  {
    "poem_id": 10917,
    "author": "Ахматова Анна Андреевна",
    "title": "Древний город словно вымер…"
  },
  {
    "poem_id": 10918,
    "author": "Ахматова Анна Андреевна",
    "title": "Знаю, знаю – снова лыжи…"
  },
  {
    "poem_id": 10919,
    "author": "Ахматова Анна Андреевна",
    "title": "Последний тост"
  },
  {
    "poem_id": 10920,
    "author": "Ахматова Анна Андреевна",
    "title": "Я не любила с давних дней…"
  },
  {
    "poem_id": 10921,
    "author": "Ахматова Анна Андреевна",
    "title": "Клятва"
  },
  {
    "poem_id": 10922,
    "author": "Ахматова Анна Андреевна",
    "title": "Воронеж"
  },
  {
    "poem_id": 10923,
    "author": "Ахматова Анна Андреевна",
    "title": "Тот голос, с тишиной великой споря…"
  },
  {
    "poem_id": 10924,
    "author": "Ахматова Анна Андреевна",
    "title": "Столько просьб у любимой всегда…"
  },
  {
    "poem_id": 10925,
    "author": "Ахматова Анна Андреевна",
    "title": "Я научилась просто, мудро жить…"
  },
  {
    "poem_id": 10926,
    "author": "Ахматова Анна Андреевна",
    "title": "Разлука"
  },
  {
    "poem_id": 10927,
    "author": "Ахматова Анна Андреевна",
    "title": "Веет ветер лебединый…"
  },
  {
    "poem_id": 10928,
    "author": "Ахматова Анна Андреевна",
    "title": "В ту ночь мы сошли друг от друга с ума…"
  },
  {
    "poem_id": 10929,
    "author": "Ахматова Анна Андреевна",
    "title": "Судьба ли так моя переменилась…"
  },
  {
    "poem_id": 10930,
    "author": "Ахматова Анна Андреевна",
    "title": "9 декабря 1913 года"
  },
  {
    "poem_id": 10931,
    "author": "Ахматова Анна Андреевна",
    "title": "Прогулка (Перо задело о верх экипажа…)"
  },
  {
    "poem_id": 10932,
    "author": "Ахматова Анна Андреевна",
    "title": "Еще весна таинственная млела…"
  },
  {
    "poem_id": 10933,
    "author": "Ахматова Анна Андреевна",
    "title": "Муза ушла по дороге…"
  },
  {
    "poem_id": 10934,
    "author": "Ахматова Анна Андреевна",
    "title": "Нет, царевич, я не та…"
  },
  {
    "poem_id": 10935,
    "author": "Ахматова Анна Андреевна",
    "title": "Читая «Гамлета»"
  },
  {
    "poem_id": 10936,
    "author": "Ахматова Анна Андреевна",
    "title": "Царскосельская ода"
  },
  {
    "poem_id": 10937,
    "author": "Ахматова Анна Андреевна",
    "title": "Вечерний звон у стен монастыря…"
  },
  {
    "poem_id": 10938,
    "author": "Ахматова Анна Андреевна",
    "title": "Читатель"
  },
  {
    "poem_id": 10939,
    "author": "Ахматова Анна Андреевна",
    "title": "Когда в тоске самоубийства…"
  },
  {
    "poem_id": 10940,
    "author": "Ахматова Анна Андреевна",
    "title": "Победителям"
  },
  {
    "poem_id": 10941,
    "author": "Ахматова Анна Андреевна",
    "title": "И упало каменное слово…"
  },
  {
    "poem_id": 10942,
    "author": "Ахматова Анна Андреевна",
    "title": "Как вплелась в мои темные косы"
  },
  {
    "poem_id": 10943,
    "author": "Ахматова Анна Андреевна",
    "title": "Муж хлестал меня узорчатым…"
  },
  {
    "poem_id": 10944,
    "author": "Ахматова Анна Андреевна",
    "title": "Он длится без конца – янтарный, тяжкий день…"
  },
  {
    "poem_id": 10945,
    "author": "Ахматова Анна Андреевна",
    "title": "Явление луны"
  },
  {
    "poem_id": 10946,
    "author": "Ахматова Анна Андреевна",
    "title": "О, знала ль я, когда в одежде белой…"
  },
  {
    "poem_id": 10947,
    "author": "Ахматова Анна Андреевна",
    "title": "Заклинание"
  },
  {
    "poem_id": 10948,
    "author": "Ахматова Анна Андреевна",
    "title": "Смуглый отрок бродил по аллеям…"
  },
  {
    "poem_id": 10949,
    "author": "Ахматова Анна Андреевна",
    "title": "Безвольно пощады просят…"
  },
  {
    "poem_id": 10950,
    "author": "Ахматова Анна Андреевна",
    "title": "Я живу, как кукушка в часах…"
  },
  {
    "poem_id": 10951,
    "author": "Ахматова Анна Андреевна",
    "title": "Пленник чужой! Мне чужого не надо…"
  },
  {
    "poem_id": 10952,
    "author": "Ахматова Анна Андреевна",
    "title": "Когда в мрачнейшей из столиц…"
  },
  {
    "poem_id": 10953,
    "author": "Ахматова Анна Андреевна",
    "title": "Надпись на неоконченном портрете"
  },
  {
    "poem_id": 10954,
    "author": "Ахматова Анна Андреевна",
    "title": "То ли я с тобой осталась…"
  },
  {
    "poem_id": 10955,
    "author": "Ахматова Анна Андреевна",
    "title": "Стансы"
  },
  {
    "poem_id": 10956,
    "author": "Ахматова Анна Андреевна",
    "title": "Не с теми я, кто бросил землю…"
  },
  {
    "poem_id": 10957,
    "author": "Ахматова Анна Андреевна",
    "title": "Не прислал ли лебедя за мною…"
  },
  {
    "poem_id": 10958,
    "author": "Ахматова Анна Андреевна",
    "title": "Я написала слова…"
  },
  {
    "poem_id": 10959,
    "author": "Ахматова Анна Андреевна",
    "title": "Под навесом темной риги жарко…"
  },
  {
    "poem_id": 10960,
    "author": "Ахматова Анна Андреевна",
    "title": "Приду туда, и отлетит томленье…"
  },
  {
    "poem_id": 10961,
    "author": "Ахматова Анна Андреевна",
    "title": "Да, я любила их, те сборища ночные…"
  },
  {
    "poem_id": 10962,
    "author": "Ахматова Анна Андреевна",
    "title": "А человек, который для меня…"
  },
  {
    "poem_id": 10963,
    "author": "Ахматова Анна Андреевна",
    "title": "Целый год ты со мной неразлучен…"
  },
  {
    "poem_id": 10964,
    "author": "Ахматова Анна Андреевна",
    "title": "Словно ангел, возмутивший воду…"
  },
  {
    "poem_id": 10965,
    "author": "Ахматова Анна Андреевна",
    "title": "Ведь где-то есть простая жизнь и свет…"
  },
  {
    "poem_id": 10966,
    "author": "Ахматова Анна Андреевна",
    "title": "Долгим взглядом твоим истомленная…"
  },
  {
    "poem_id": 10967,
    "author": "Ахматова Анна Андреевна",
    "title": "Он прав – опять фонарь, аптека…"
  },
  {
    "poem_id": 10968,
    "author": "Ахматова Анна Андреевна",
    "title": "Эта встреча никем не воспета…"
  },
  {
    "poem_id": 10969,
    "author": "Ахматова Анна Андреевна",
    "title": "Клеопатра"
  },
  {
    "poem_id": 10970,
    "author": "Ахматова Анна Андреевна",
    "title": "Любовь покоряет обманно…"
  },
  {
    "poem_id": 10971,
    "author": "Ахматова Анна Андреевна",
    "title": "А Смоленская нынче именинница…"
  },
  {
    "poem_id": 10972,
    "author": "Ахматова Анна Андреевна",
    "title": "Широк и желт вечерний свет…"
  },
  {
    "poem_id": 10973,
    "author": "Ахматова Анна Андреевна",
    "title": "Ждала его напрасно много лет…"
  },
  {
    "poem_id": 10974,
    "author": "Ахматова Анна Андреевна",
    "title": "Так не зря мы вместе бедовали…"
  },
  {
    "poem_id": 10975,
    "author": "Ахматова Анна Андреевна",
    "title": "Загорелись иглы венчика…"
  },
  {
    "poem_id": 10976,
    "author": "Ахматова Анна Андреевна",
    "title": "Под крышей промерзшей пустого жилья…"
  },
  {
    "poem_id": 10977,
    "author": "Ахматова Анна Андреевна",
    "title": "Вот это я тебе, взамен могильных роз…"
  },
  {
    "poem_id": 10978,
    "author": "Ахматова Анна Андреевна",
    "title": "Deprofundis … Мое поколенье…"
  },
  {
    "poem_id": 10979,
    "author": "Ахматова Анна Андреевна",
    "title": "Молитва"
  },
  {
    "poem_id": 10980,
    "author": "Ахматова Анна Андреевна",
    "title": "Клевета (И всюду клевета сопутствовала мне…)"
  },
  {
    "poem_id": 10981,
    "author": "Ахматова Анна Андреевна",
    "title": "Этой ивы листы в девятнадцатом веке увяли…"
  },
  {
    "poem_id": 10982,
    "author": "Ахматова Анна Андреевна",
    "title": "Вечерние часы перед столом…"
  },
  {
    "poem_id": 10983,
    "author": "Ахматова Анна Андреевна",
    "title": "Для того ль тебя носила…"
  },
  {
    "poem_id": 10984,
    "author": "Ахматова Анна Андреевна",
    "title": "Сладок запах синих виноградин…"
  },
  {
    "poem_id": 10985,
    "author": "Ахматова Анна Андреевна",
    "title": "Первое возвращение"
  },
  {
    "poem_id": 10986,
    "author": "Ахматова Анна Андреевна",
    "title": "Муза"
  },
  {
    "poem_id": 10987,
    "author": "Ахматова Анна Андреевна",
    "title": "Ты всегда таинственный и новый…"
  },
  {
    "poem_id": 10988,
    "author": "Ахматова Анна Андреевна",
    "title": "Божий Ангел, зимним утром…"
  },
  {
    "poem_id": 10989,
    "author": "Ахматова Анна Андреевна",
    "title": "Весенним солнцем это утро пьяно…"
  },
  {
    "poem_id": 10990,
    "author": "Ахматова Анна Андреевна",
    "title": "Многим"
  },
  {
    "poem_id": 10991,
    "author": "Ахматова Анна Андреевна",
    "title": "Покорно мне воображенье…"
  },
  {
    "poem_id": 10992,
    "author": "Ахматова Анна Андреевна",
    "title": "Бесшумно ходили по дому…"
  },
  {
    "poem_id": 10993,
    "author": "Ахматова Анна Андреевна",
    "title": "Я не знаю, ты жив или умер…"
  },
  {
    "poem_id": 10994,
    "author": "Ахматова Анна Андреевна",
    "title": "Забудут – вот чем удивили…"
  },
  {
    "poem_id": 10995,
    "author": "Ахматова Анна Андреевна",
    "title": "Памяти Сергея Есенина"
  },
  {
    "poem_id": 10996,
    "author": "Ахматова Анна Андреевна",
    "title": "Щели в саду вырыты…"
  },
  {
    "poem_id": 10997,
    "author": "Ахматова Анна Андреевна",
    "title": "Как площади эти обширны…"
  },
  {
    "poem_id": 10998,
    "author": "Ахматова Анна Андреевна",
    "title": "Тебе покорной Ты сошел с ума…"
  },
  {
    "poem_id": 10999,
    "author": "Ахматова Анна Андреевна",
